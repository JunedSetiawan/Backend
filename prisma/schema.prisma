generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model siswa {
  id              Int       @id @default(autoincrement())
  nama            String
  kelas           String
  tanggal_lahir   DateTime? @db.Date
  ekstrakurikuler String?
  createdAt       DateTime  @default(now())
  updateAt        DateTime? @updatedAt
}
